{
  "name": "dumd",
  "version": "1.0.0",
  "description": "dumb umd loading",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist && mkdir dist",
    "watch": "chokidar 'src/**/*' -c 'npm run build:dev'",
    "build:dev": "npm run clean && buble src/index.js > dist/index.js",
    "build": "npm run clean && buble src/index.js | uglifyjs --compress --mangle > dist/index.js",
    "dev": "open $PWD/example/index.html && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skiano/dumd.git"
  },
  "keywords": [
    "umd",
    "tooling",
    "loader"
  ],
  "author": "Greg Skiano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skiano/dumd/issues"
  },
  "homepage": "https://github.com/skiano/dumd#readme",
  "devDependencies": {
    "buble": "^0.18.0",
    "chokidar-cli": "^1.2.0",
    "nodemon": "^1.14.12",
    "serve": "^6.4.9",
    "uglify-js": "^3.3.10"
  }
}
